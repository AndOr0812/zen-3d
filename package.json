{
    "name": "zen-3d",
    "version": "0.0.0",
    "description": "JavaScript 3D library",
    "main": "build/zen3d.js",
    "directories": {
        "doc": "docs",
        "example": "examples",
        "test": "test"
    },
    "keywords": [
        "zen3d",
        "zen-3d",
        "3d",
        "webgl"
    ],
    "author": "shawn0326",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/shawn0326/zen-3d/issues"
    },
    "dependencies": {},
    "repository": {},
    "devDependencies": {
        "gulp": "^3.9.1",
        "gulp-concat": "^2.6.1",
        "gulp-notify": "^3.0.0",
        "gulp-rename": "^1.3.0",
        "gulp-uglify": "^3.0.0",
        "live-server": "^1.2.0",
        "rollup": "^0.60.4",
        "uglify-js": "^3.0.23"
    },
    "scripts": {
        "default": "npm run build",
        "b": "npm run build",
        "build": "rollup -c",
        "uglify": "uglifyjs build/zen3d.js -cm --preamble \"xxxxxxxxxx\" > build/zen3d.min.js",
        "build-uglify": "rollup -c && uglifyjs ./build/zen3d.js -cm --preamble 1 > ./build/zen3d.min.js",
        "extension": "gulp extension",
        "example": "live-server --port=8080 --open=examples --watch=examples,build"
    }
